<html>
  <head>
    <meta charset="UTF-8" />
    <title>MySQL Dump of <%= @table.table_name.titlecase %></title>
  </head>
  <body>
    <h1><%= @table.table_name.titlecase %></h1>
    <table>
      <tr>
        <% @table.column_names.each do |col| %>
          <th><%= col %></th>
        <% end %>
      </tr>
      <% @print_data.each do |row| %>
        <tr>
        <% @table.column_names.each do |col| %>
          <td><%= row[col] %></td>
        <% end %>
        </tr>
      <% end %>
      <tr>
        <th></th>
      </tr>
    </table>
  </body>
</html>
